{
  "name": "react-simplified-ui",
  "version": "1.0.0",
  "description": "react simple component library",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "files": [
    "build"
  ],
  "scripts": {
    "start": "npm run storybook",
    "build": "rollup -c",
    "test": "jest --watch",
    "test:once": "jest",
    "format": "prettier --write ./src/**/*.{ts,tsx}",
    "lint": "eslint --fix ./src/**/*.{js,ts,tsx}",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saijo-shota-biz/react-simplified-ui.git"
  },
  "keywords": [
    "React",
    "Typescript",
    "Component",
    "Library",
    "Sass",
    "Storybook"
  ],
  "author": "saijo-shota-biz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saijo-shota-biz/react-simplified-ui/issues"
  },
  "homepage": "https://github.com/saijo-shota-biz/react-simplified-ui#readme",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-preset-react-app": "^9.1.2",
    "@babel/core": "^7.9.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "rollup": "^2.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-typescript2": "^0.27.0",
    "sass-loader": "^8.0.2",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@types/storybook__addon-info": "^5.2.1",
    "@types/storybook__addon-knobs": "^5.2.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "rollup-plugin-copy": "^3.3.0"
  }
}
